{
	"name": "got-pool",
	"version": "0.1.0",
	"engines": {
		"node": "10.15.3"
	},
	"private": true,
	"dependencies": {
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"react-howler": "^3.7.4",
		"react-scripts": "2.1.8",
		"morgan": "^1.8.2",
		"npm": "^6.4.1",
		"body-parser": "^1.17.2",
		"dotenv": "^4.0.0",
		"express": "^4.15.3",
		"mongoose": "^4.11.4",
		"axios": "^0.16.2"
	},
	"devDependencies": {
		"concurrently": "^3.5.0",
		"react-scripts": "1.0.7"
	},
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "npm run client",
		"client": "cd client && npm run start",
		"install": "cd client && npm install",
		"build": "cd client && npm run build",
		"heroku-postbuild": "npm run build",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"server": "nodemon server.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
